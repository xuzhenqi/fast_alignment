layer {
  name: "upsample_reshape"
  type: "Reshape"
  bottom: "conv3_3"
  top: "upsample_reshape"
  reshape_param {
    shape {
      dim: 0
      dim: 0
      dim: -1
    }
  }
}
layer {
  name: "loss"
  type: "SoftmaxEntropyLoss"
  bottom: "upsample_reshape"
  bottom: "label4"
  top: "loss"
  softmax_param {
    axis: 2
  }
}
layer {
  name: "error"
  type: "FaceDetectionAccuracy"
  bottom: "upsample_reshape"
  bottom: "label4"
  top: "errrate"
}
